{
    "@context": [
        "http://www.w3.org/ns/odrl.jsonld",
        {
            "dc": "http://purl.org/dc/terms/",
            "ids": "https://w3id.org/idsa/core/",
            "idsc": "https://w3id.org/idsa/code/"
        }
    ],
    "@type": "Agreement",
    "permission": [
        {
            "action": "use",
            "assignee": "http://example.com/ids/party/consumer-party",
            "assigner": "http://example.com/party/my-party",
            "duty": [
                {
                    "action": [
                        {
                            "rdf:value": {
                                "@id": "replace"
                            },
                            "refinement": [
                                {
                                    "leftOperand": "idsc:REPLACE_WITH",
                                    "operator": "eq",
                                    "rightOperand": {
                                        "@type": "xsd:string",
                                        "@value": "ValueToChange"
                                    }
                                },
                                {
                                    "leftOperand": "idsc:JSON_PATH",
                                    "operator": "eq",
                                    "rightOperand": {
                                        "@type": "xsd:string",
                                        "@value": "FieldToChange"
                                    }
                                }
                            ]
                        }
                    ],
                    "constraint": [
                        {
                            "leftOperand": "event",
                            "operator": "lt",
                            "rightOperand": {
                                "@id": "odrl:policyUsage"
                            }
                        }
                    ]
                }
            ],
            "target": "http://target/"
        }
    ],
    "profile": "http://example.com/ids-profile",
    "uid": "https://w3id.org/idsa/autogen/contract/anonymize-in-transit"
}